{
  "server.default_cache_timeout": "15m",
  "server.force_local_cache": false,
  "server.enableMTOM": "optional",
  "user_store.type": "database",
  "super_admin.admin_role": "admin",
  "authorization_manager": {
    "class": "org.wso2.carbon.user.core.authorization.JDBCAuthorizationManager",
    "properties": {
      "AdminRoleManagementPermissions": "/permission",
      "AuthorizationCacheEnabled": "true",
      "GetAllRolesOfUserEnabled": "false"
    }
  },
  "realm_manager": {
    "data_source": "SHARED_DB",
    "properties": {
      "isCascadeDeleteEnabled": true
    }
  },
  "user_store.properties.ReadGroups": true,
  "user_store.properties.UsernameJavaScriptRegEx": "^[\\S]{5,30}$",
  "user_store.properties.UsernameJavaRegExViolationErrorMsg": "Username pattern policy violated",
  "user_store.properties.PasswordJavaRegEx": "^[\\S]{5,30}$",
  "user_store.properties.PasswordJavaScriptRegEx": "^[\\S]{5,30}$",
  "user_store.properties.PasswordJavaRegExViolationErrorMsg": "Password length should be within 5 to 30 characters",
  "user_store.properties.RolenameJavaScriptRegEx": "^[\\S]{3,30}$",
  "user_store.properties.MultiAttributeSeparator": ",",
  "user_store.properties.MaxUserNameListLength": "100",
  "user_store.properties.MaxRoleNameListLength": "100",
  "governance_data.data_source": "SHARED_DB",
  "governance_data.cache_id": "$ref{database.shared_db.username}@$ref{database.shared_db.url}",
  "governance_data.enable_cache": true,
  "governance_data.path": "/_system/governance",
  "config_data.path": "/_system/config",
  "config_data.data_source": "$ref{governance_data.data_source}",
  "config_data.cache_id": "$ref{governance_data.cache_id}",
  "config_data.enable_cache": "$ref{governance_data.enable_cache}",
  "transport.http.port": "9763",
  "transport.http.redirectPort": "9443",
  "transport.http.bindOnInit": "false",
  "transport.http.maxHttpHeaderSize": "8192",
  "transport.http.acceptorThreadCount": "2",
  "transport.http.maxThreads": "250",
  "transport.http.minSpareThreads": "50",
  "transport.http.disableUploadTimeout": "false",
  "transport.http.connectionUploadTimeout": "120000",
  "transport.http.maxKeepAliveRequests": "200",
  "transport.http.acceptCount": "200",
  "transport.http.server": "WSO2 Carbon Server",
  "transport.http.compression": "on",
  "transport.http.compressionMinSize": "2048",
  "transport.http.noCompressionUserAgents": "gozilla, traviata",
  "transport.http.compressableMimeType": "text/html,text/javascript,application/x-javascript,application/javascript,application/xml,text/css,application/xslt+xml,text/xsl,image/gif,image/jpg,image/jpeg",
  "transport.http.URIEncoding": "UTF-8",
  "transport.https.port": "9443",
  "transport.https.bindOnInit": "false",
  "transport.https.sslProtocol": "TLS",
  "transport.https.sslEnabledProtocols": "TLSv1,TLSv1.1,TLSv1.2",
  "transport.https.maxHttpHeaderSize": "8192",
  "transport.https.acceptorThreadCount": "2",
  "transport.https.maxThreads": "250",
  "transport.https.minSpareThreads": "50",
  "transport.https.disableUploadTimeout": "false",
  "transport.https.enableLookups": "false",
  "transport.https.connectionUploadTimeout": "120000",
  "transport.https.maxKeepAliveRequests": "200",
  "transport.https.acceptCount": "200",
  "transport.https.server": "WSO2 Carbon Server",
  "transport.https.clientAuth": "false",
  "transport.https.compression": "on",
  "transport.https.scheme": "https",
  "transport.https.secure": "true",
  "transport.https.SSLEnabled": "true",
  "transport.https.compressionMinSize": "2048",
  "transport.https.noCompressionUserAgents": "gozilla, traviata",
  "transport.https.compressableMimeType": "text/html,text/javascript,application/x-javascript,application/javascript,application/xml,text/css,application/xslt+xml,text/xsl,image/gif,image/jpg,image/jpeg",
  "transport.https.keystoreFile": "${carbon.home}/repository/resources/security/$ref{keystore.tls.file_name}",
  "transport.https.keystorePass": "$ref{keystore.tls.password}",
  "transport.https.URIEncoding": "UTF-8",
  "message_formatter.form_urlencoded": "org.apache.axis2.transport.http.XFormURLEncodedFormatter",
  "message_formatter.multipart_form_data": "org.apache.axis2.transport.http.MultipartFormDataFormatter",
  "message_formatter.application_xml": "org.apache.axis2.transport.http.ApplicationXMLFormatter",
  "message_formatter.text_xml": "org.apache.axis2.transport.http.SOAPMessageFormatter",
  "message_formatter.soap_xml": "org.apache.axis2.transport.http.SOAPMessageFormatter",
  "message_formatter.json": "org.apache.axis2.json.gson.JsonFormatter",
  "message_formatter.json_badgerfish": "org.apache.axis2.json.JSONBadgerfishMessageFormatter",
  "message_formatter.text_javascript": "org.apache.axis2.json.gson.JsonFormatter",
  "message_builder.application_xml": "org.apache.axis2.builder.ApplicationXMLBuilder",
  "message_builder.form_urlencoded": "org.apache.axis2.builder.XFormURLEncodedBuilder",
  "message_builder.multipart_form_data": "org.apache.axis2.builder.MultipartFormDataBuilder",
  "message_builder.json": "org.apache.axis2.json.gson.JsonBuilder",
  "message_builder.json_badgerfish": "org.apache.axis2.json.JSONBadgerfishOMBuilder",
  "message_builder.text_javascript": "org.apache.axis2.json.gson.JsonBuilder",
  "clustering.domain": "wso2.carbon.domain",
  "clustering.local_member_host": "$ref{server.node_ip}",
  "clustering.members": [
    "127.0.0.1:4000"
  ],
  "clustering.local_member_port": "4000",
  "database_configuration.enable_h2_db": false,
  "wsdl.skip_null_elements": false,
  "database.shared_db.id": "SHARED_DB",
  "database.shared_db.pool_options.maxActive": "50",
  "database.shared_db.pool_options.maxWait": "60000",
  "database.shared_db.pool_options.testOnBorrow": "true",
  "database.shared_db.pool_options.validationInterval": "30000",
  "database.shared_db.pool_options.defaultAutoCommit": "true",
  "keystore.tls.file_name": "wso2carbon.jks",
  "keystore.tls.type": "JKS",
  "keystore.tls.password": "wso2carbon",
  "keystore.tls.alias": "wso2carbon",
  "keystore.tls.key_password": "wso2carbon",
  "keystore.internal.file_name": "$ref{keystore.tls.file_name}",
  "keystore.internal.type": "$ref{keystore.tls.type}",
  "keystore.internal.password": "$ref{keystore.tls.password}",
  "keystore.internal.alias": "$ref{keystore.tls.alias}",
  "keystore.internal.key_password": "$ref{keystore.tls.key_password}",
  "truststore.file_name": "client-truststore.jks",
  "truststore.type": "JKS",
  "truststore.password": "wso2carbon",
  "encryption.type": "asymmetric",
  "encryption.algorithm": "AES",
  "encryption.alias": "symmetric.key.value",
  "encryption.internal_crypto_provider": "org.wso2.carbon.crypto.provider.KeyStoreBasedInternalCryptoProvider",
  "admin_console.authenticator.token_ui_authenticator.enable": false,
  "admin_console.authenticator.token_ui_authenticator.priority": "5",
  "admin_console.authenticator.saml_sso_authenticator.enable": false,
  "admin_console.authenticator.saml_sso_authenticator.priority": "10",
  "admin_console.authenticator.saml_sso_authenticator.config.LoginPage": "/carbon/admin/login.jsp",
  "admin_console.authenticator.saml_sso_authenticator.config.ServiceProviderID": "carbonServer",
  "admin_console.authenticator.saml_sso_authenticator.config.IdentityProviderSSOServiceURL": "$ref{server.base_path}/samlsso",
  "admin_console.authenticator.saml_sso_authenticator.config.NameIDPolicyFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified",
  "admin_console.authenticator.saml_sso_authenticator.config.AssertionConsumerServiceURL": "$ref{server.base_path}/acs",
  "admin_console.authenticator.saml_sso_authenticator.config.JITUserProvisioningEnabled": false,
  "admin_console.authenticator.saml_sso_authenticator.config.IsSuperAdminRoleRequired": false,
  "admin_console.authenticator.mutual_ssl_authenticator.priority": "5",
  "admin_console.authenticator.mutual_ssl_authenticator.enable": false,
  "admin_console.authenticator.mutual_ssl_authenticator.config.UsernameHeader": "UserName",
  "admin_console.authenticator.mutual_ssl_authenticator.config.WhiteListEnabled": false,
  "owasp.csrfguard.create_token_per_page": false,
  "owasp.csrfguard.token_length": "32",
  "owasp.csrfguard.random_number_generator_algo": "SHA1PRNG",
  "owasp.csrfguard.js_servlet.x_request_with_header": "WSO2 CSRF Protection",
  "tomcat.global.session_timeout": "30m",
  "tomcat.management_console.session_timeout": "15m",

  "carbon_health_check.enable": false,
  "carbon_health_check.health_checker.data_source_health_checker.order": "97",
  "carbon_health_check.health_checker.data_source_health_checker.enable": true,
  "carbon_health_check.health_checker.super_tenant_health_checker.order": "98",
  "carbon_health_check.health_checker.super_tenant_health_checker.enable": true,
  "carbon_health_check.health_checker.data_source_health_checker.pool_usage_limit_percentage": "80",

  "tasks.mode": "AUTO",
  "tasks.client_dispatch_address": "https://$ref{server.hostname}:9448",
  "tasks.server_address": "https://$ref{server.hostname}:9443",
  "tasks.server_count": "1",
  "tasks.server_username": "admin",
  "tasks.server_password": "admin",
  "tasks.resolver_class": "org.wso2.carbon.ntask.core.impl.RoundRobinTaskLocationResolver",
  "server.base_path" : "${carbon.protocol}://${carbon.host}:${carbon.management.port}",
  "system.parameter": {
    "org.wso2.CipherTransformation": "RSA/ECB/OAEPwithSHA1andMGF1Padding"
  }
}
