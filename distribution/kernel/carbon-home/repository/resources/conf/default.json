{
  "server.default_cache_timeout": "15m",
  "server.force_local_cache": true,
  "server.discard_empty_caches": false,
  "server.enableMTOM": "optional",
  "server.file_upload.file_size_limit": "100",
  "user_store.type": "database",
  "super_admin.admin_role": "admin",
  "authorization_manager": {
    "class": "org.wso2.carbon.user.core.authorization.JDBCAuthorizationManager",
    "properties": {
      "AdminRoleManagementPermissions": "/permission",
      "AuthorizationCacheEnabled": "true",
      "GetAllRolesOfUserEnabled": "false"
    }
  },
  "realm_manager": {
    "data_source": "SHARED_DB",
    "properties": {
      "isCascadeDeleteEnabled": true
    }
  },
  "user_store.properties.ReadGroups": true,
  "user_store.properties.UsernameJavaScriptRegEx": "^[\\S]{3,30}$",
  "user_store.properties.UsernameJavaRegExViolationErrorMsg": "Username pattern policy violated",
  "user_store.properties.PasswordJavaRegEx": "^[\\S]{5,30}$",
  "user_store.properties.PasswordJavaScriptRegEx": "^[\\S]{5,30}$",
  "user_store.properties.PasswordJavaRegExViolationErrorMsg": "Password length should be within 5 to 30 characters",
  "user_store.properties.RolenameJavaScriptRegEx": "^[\\S]{3,30}$",
  "user_store.properties.MultiAttributeSeparator": ",",
  "user_store.properties.MaxUserNameListLength": "100",
  "user_store.properties.MaxRoleNameListLength": "100",
  "governance_data.data_source": "SHARED_DB",
  "governance_data.cache_id": "$ref{database.shared_db.username}@$ref{database.shared_db.url}",
  "governance_data.enable_cache": true,
  "governance_data.path": "/_system/governance",
  "config_data.path": "/_system/config",
  "config_data.data_source": "$ref{governance_data.data_source}",
  "config_data.cache_id": "$ref{governance_data.cache_id}",
  "config_data.enable_cache": "$ref{governance_data.enable_cache}",
  "transport.http.properties.port": "9763",
  "transport.http.properties.redirectPort": "9443",
  "transport.http.properties.bindOnInit": "false",
  "transport.http.properties.maxHttpHeaderSize": "8192",
  "transport.http.properties.acceptorThreadCount": "2",
  "transport.http.properties.maxThreads": "250",
  "transport.http.properties.minSpareThreads": "50",
  "transport.http.properties.disableUploadTimeout": "false",
  "transport.http.properties.connectionUploadTimeout": "120000",
  "transport.http.properties.maxKeepAliveRequests": "200",
  "transport.http.properties.acceptCount": "200",
  "transport.http.properties.server": "WSO2 Carbon Server",
  "transport.http.properties.compression": "on",
  "transport.http.properties.compressionMinSize": "2048",
  "transport.http.properties.noCompressionUserAgents": "gozilla, traviata",
  "transport.http.properties.compressibleMimeType": "text/html,text/javascript,application/x-javascript,application/javascript,application/xml,text/css,application/xslt+xml,text/xsl,image/gif,image/jpg,image/jpeg",
  "transport.http.properties.URIEncoding": "UTF-8",
  "transport.https.properties.port": "9443",
  "transport.https.properties.bindOnInit": "false",
  "transport.https.properties.maxHttpHeaderSize": "8192",
  "transport.https.properties.acceptorThreadCount": "2",
  "transport.https.properties.maxThreads": "250",
  "transport.https.properties.minSpareThreads": "50",
  "transport.https.properties.disableUploadTimeout": "false",
  "transport.https.properties.enableLookups": "false",
  "transport.https.properties.connectionUploadTimeout": "120000",
  "transport.https.properties.maxKeepAliveRequests": "200",
  "transport.https.properties.acceptCount": "200",
  "transport.https.properties.server": "WSO2 Carbon Server",
  "transport.https.properties.compression": "on",
  "transport.https.properties.scheme": "https",
  "transport.https.properties.secure": "true",
  "transport.https.properties.SSLEnabled": "true",
  "transport.https.properties.compressionMinSize": "2048",
  "transport.https.properties.noCompressionUserAgents": "gozilla, traviata",
  "transport.https.properties.compressibleMimeType": "text/html,text/javascript,application/x-javascript,application/javascript,application/xml,text/css,application/xslt+xml,text/xsl,image/gif,image/jpg,image/jpeg",
  "transport.https.properties.URIEncoding": "UTF-8",
  "transport.https.sslHostConfig.properties.protocols": "+TLSv1,+TLSv1.1,+TLSv1.2",
  "transport.https.sslHostConfig.properties.sslProtocol": "TLS",
  "transport.https.sslHostConfig.properties.certificateVerification": "false",
  "transport.https.sslHostConfig.properties.truststoreFile": "${carbon.home}/repository/resources/security/$ref{truststore.file_name}",
  "transport.https.sslHostConfig.properties.truststorePassword": "$ref{truststore.password}",
  "transport.https.sslHostConfig.properties.truststoreType": "$ref{truststore.type}",
  "transport.https.sslHostConfig.certificate.properties.certificateKeystoreFile": "${carbon.home}/repository/resources/security/$ref{keystore.tls.file_name}",
  "transport.https.sslHostConfig.certificate.properties.certificateKeystorePassword": "$ref{keystore.tls.password}",
  "transport.https.sslHostConfig.certificate.properties.certificateKeystoreType": "$ref{keystore.tls.type}",
  "transport.https.sslHostConfig.certificate.properties.certificateKeyAlias": "$ref{keystore.tls.alias}",
  "transport.https.sslHostConfig.certificate.properties.certificateKeyPassword": "$ref{keystore.tls.key_password}",
